money = int(input('Введите сумму вклада: '))# запрашиваем размер депозита
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0} # словарь - названия банков и процентные ставки по депозитам
deposit = [] # список процентных ставок
for key in per_cent:
    deposit.append(per_cent[key] * money / 100) # вычисляем размер накопленных процентов на сумму согласно указанной ставки банка
print(deposit)
max_deposit = max(deposit) # максимальный депозит 
print ('Максимальная сумма, которую вы можете заработать: ' + str(max_deposit))
